{
  "name": "ghsa-network-analysis",
  "version": "0.0.0",
  "description": "A network analysis of funding in global health security.",
  "main": "server.js",
  "watch": {
    "start": "./server.js"
  },
  "scripts": {
    "start": "node --max_old_space_size=6000 server.js",
    "test": "npm-watch start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/talus-analytics/ghsa-network-analysis.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/talus-analytics/ghsa-network-analysis/issues"
  },
  "homepage": "https://github.com/talus-analytics/ghsa-network-analysis#readme",
  "devDependencies": {
    "babel-preset-es2015": "^6.6.0",
    "eslint": "~2.5.3",
    "eslint-config-airbnb": "~6.2.0",
    "gulp": "^3.9.1",
    "gulp-babel": "~6.1.2",
    "gulp-concat": "~2.6.0",
    "gulp-eslint": "^2.0.0",
    "gulp-rename": "~1.2.2",
    "gulp-uglify": "~1.5.1",
    "npm-watch": "^0.6.0"
  },
  "eslintConfig": {
    "extends": "airbnb/base",
    "env": {
      "jquery": true
    },
    "globals": {
      "App": true,
      "Routing": true,
      "Util": true,
      "Map": true,
      "hasher": true,
      "crossroads": true,
      "Handlebars": true,
      "d3": true,
      "L": true,
      "noty": true,
      "topojson": true,
      "NProgress": true,
      "Fuse": true
    },
    "rules": {
      "guard-for-in": 0,
      "new-cap": 0,
      "indent": [
        "error",
        "tab"
      ],
      "no-confusing-arrow": [
        "error",
        {
          "allowParens": true
        }
      ],
      "no-use-before-define": [
        "error",
        "nofunc"
      ],
      "no-param-reassign": [
        "error",
        {
          "props": false
        }
      ]
    }
  },
  "dependencies": {
    "@google-cloud/translate": "^1.0.0",
    "aws-sdk": "^2.426.0",
    "body-parser": "^1.19.0",
    "cookieconsent": "^3.1.0",
    "eslint-plugin-react": "^4.3.0",
    "express": "^4.15.4",
    "formidable": "^1.1.1",
    "google-translate-api": "^2.3.0",
    "jfile": "^1.1.12",
    "jsontocsv": "^0.3.0",
    "xlsx-populate": "^1.19.1"
  }
}
